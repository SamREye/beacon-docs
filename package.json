{
  "name": "beacon-docs",
  "version": "0.0.0",
  "scripts": {
    "actions:deploy": "ng deploy --name $GITHUB_ACTOR --email $GITHUB_ACTOR@users.noreply.github.com --no-silent",
    "actions:prettier": "prettier \"src/**/*.ts\" \"e2e/**/*.ts\" \"angular.json\" \"README.md\" --list-different",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "e2e": "ng e2e",
    "copy-examples": "node scripts/copy-examples.js",
    "generate-definitions": "node scripts/generate-definitions.js",
    "generate-files": "npm run copy-examples && npm run generate-definitions",
    "postinstall": "node scripts/patch-crypto.js && npm run generate-files && npm run prepare-prod-build",
    "prettier": "prettier --write \"src/**/*.ts\" \"e2e/**/*.ts\" \"angular.json\" \"README.md\"",
    "prepare-prod-build": "npm run disable-pure-getters && npm run configure-mangle",
    "disable-pure-getters": "replace 'pure_getters: buildOptions.buildOptimizer' 'pure_getters: false' ./node_modules/@angular-devkit/build-angular/src/webpack/configs/common.js ",
    "configure-mangle": "replace 'safari10: true' 'safari10: true, keep_fnames: true' ./node_modules/@angular-devkit/build-angular/src/webpack/configs/common.js "
  },
  "private": true,
  "dependencies": {
    "@airgap/beacon-sdk": "^2.2.2",
    "@angular/animations": "~11.2.5",
    "@angular/common": "~11.2.5",
    "@angular/compiler": "~11.2.5",
    "@angular/core": "~11.2.5",
    "@angular/forms": "~11.2.5",
    "@angular/localize": "~11.2.5",
    "@angular/platform-browser": "~11.2.5",
    "@angular/platform-browser-dynamic": "~11.2.5",
    "@angular/router": "~11.2.5",
    "@materia-ui/ngx-monaco-editor": "^5.0.0",
    "@ng-bootstrap/ng-bootstrap": "^9.0.2",
    "@taquito/beacon-wallet": "^8.0.6-beta.0",
    "@taquito/taquito": "^8.0.6-beta.0",
    "bootstrap": "^4.5.0",
    "dompurify": "^2.2.7",
    "highlight.js": "^10.6.0",
    "marked": "^2.0.1",
    "monaco-editor": "^0.23.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.3",
    "@angular/cli": "~11.2.4",
    "@angular/compiler-cli": "~11.2.5",
    "@types/dompurify": "^2.2.1",
    "@types/jasmine": "~3.6.0",
    "@types/marked": "^2.0.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "prettier": "^2.2.1",
    "protractor": "~7.0.0",
    "raw-loader": "^4.0.2",
    "replace": "^1.2.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
